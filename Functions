#ARITHMETIC OPERATIONS
def add(x,y):
    print(x+y)
def sub(x,y):
    print(x-y)
def mul(x,y):
    print(x*y)
def mod(x,y):
    print(x/y)
def fd(x,y):
    print(x//y)
def rem(x,y):
    print(x%y)
a=int(input())
b=int(input())
add(a,b)
sub(a,b)
mul(a,b)
mod(a,b)
fd(a,b)
rem(a,b)


#SQUARE ROOT
def sqr(x):
    print(x**2)
a=int(input())
sqr(a)


#CUBE ROOT
def cbr(x):
    print(x**3)
a=int(input())
cbr(a)
