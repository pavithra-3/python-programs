t=float(input())
if s>t:
    print("yes")
else:
    print("no")
    
    
#BUDGET
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
p=A*350.34
q=B*230.90
r=C*190.55
s=D*125.30
t=E*180.90
z=p+q+r+s+t
if z<=15000:
    print("yes")
else:
    print("no")
    
    
#SECE DINING
x=input()
y=int(input())
if(x=='front' and y==1) or (x=='rear' and y==2):
    print("left handed")
elif(x=='front' and y==2) or (x=='rear' and y==1):
    print("right handed")
else:
    print("Invalid Input")
    
    
#INTERSECTION OF CIRCLE
import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
a=math.sqrt((x1-x2)**2+(y1-y2)**2)
print(a)
if r1+r2==a:
    print("the circles are tangential")
elif r1+r2>a:
    print("the circles  overlap")
else:
    print("the circles do not overlap")
    
    
#MICROWAVE OVEN
print("Enter the number of items")
X=int(input())
print("Enter the single item heating time")
Y=float(input())
A=Y+((50/100)*Y)
B=Y+(2*Y)
if X==2:
    print("The recommended heating time is %.2f"%A)
elif X==3:
    print("The recommended heating time is %.2f"%B)
else:
    print("Number of items is more")
